    "components": {
        "schemas": {
            "Property": {
                "required": [
                    "id",
                    "name",
                    "description",
                    "price",
                    "address",
                    "landlord_id"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Beautiful House"
                    },
                    "description": {
                        "type": "string",
                        "example": "A beautiful 3-bedroom house"
                    },
                    "address": {
                        "type": "string",
                        "example": "Koinange Street"
                    },
                    "city": {
                        "type": "text",
                        "format": "float",
                        "example": "Nairobi"
                    },
                    "state": {
                        "type": "string",
                        "example": "Kenya"
                    },
                    "zip_code": {
                        "type": "string",
                        "example": "00100"
                    },
                    "features": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "example": [
                            "pool",
                            "garden"
                        ]
                    }
                },
                "type": "object"
            },
            "Unit": {
                "required": [
                    "unit_number",
                    "rent_amount",
                    "size",
                    "bedrooms",
                    "bathrooms",
                    "features",
                    "status"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "property_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "unit_number": {
                        "type": "string",
                        "example": "A101"
                    },
                    "rent_amount": {
                        "type": "number",
                        "format": "float",
                        "example": 1200
                    },
                    "size": {
                        "type": "number",
                        "example": 50
                    },
                    "bedrooms": {
                        "type": "integer",
                        "example": 2
                    },
                    "bathrooms": {
                        "type": "integer",
                        "example": 1
                    },
                    "features": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "example": [
                            "balcony",
                            "parking"
                        ]
                    },
                    "status": {
                        "type": "string",
                        "example": "available"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-05-15T12:00:00Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-05-15T12:00:00Z"
                    }
                },
                "type": "object"
            }
        }
    },
